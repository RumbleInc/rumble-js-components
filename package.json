{
  "name": "rumble-components",
  "version": "0.2.3",
  "description": "React components kit",
  "main": "lib/index.js",
  "scripts": {
    "build": "./node_modules/.bin/babel src --out-dir lib",
    "start": "./node_modules/.bin/babel --watch src --out-dir lib",
    "test": "./node_modules/.bin/jest",
    "publish-patch": "npm run build && npm version patch && npm publish",
    "publish-minor": "npm run build && npm version minor && npm publish",
    "publish-major": "npm run build && npm version major && npm publish"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/RumbleInc/rumble-js-components.git"
  },
  "keywords": [
    "react"
  ],
  "author": "Alexey Volkov",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/RumbleInc/rumble-js-components/issues"
  },
  "homepage": "https://github.com/RumbleInc/rumble-js-components#readme",
  "dependencies": {
    "bluebird": "2.9.26",
    "color": "0.7.3",
    "d3": "3.5.6",
    "iscroll": "5.1.3",
    "lodash": "3.10.1",
    "merge": "1.2.0",
    "moment": "2.10.6",
    "moment-range": "2.0.3",
    "numeral": "1.5.3",
    "papaparse": "4.1.1",
    "react-color": "1.2.1",
    "react-styler": "0.1.3",
    "reactcss": "0.3.2"
  },
  "devDependencies": {
    "babel-cli": "6.4.5",
    "babel-jest": "6.0.1",
    "babel-plugin-transform-react-jsx": "6.4.0",
    "babel-preset-es2015": "6.3.13",
    "enzyme": "2.0.0",
    "file-loader": "0.8.4",
    "jest": "0.1.40",
    "jest-cli": "0.8.2",
    "jscs": "2.9.0",
    "json-loader": "0.5.3",
    "react": "0.14.x",
    "react-addons-test-utils": "0.14.x",
    "react-dom": "0.14.x",
    "url-loader": "0.5.6"
  },
  "peerDependencies": {
    "react": "0.14.x",
    "react-dom": "0.14.x"
  },
  "jest": {
    "scriptPreprocessor": "<rootDir>/node_modules/babel-jest",
    "testDirectoryName": "tests",
    "testPathDirs": [
      "<rootDir>/tests/components"
    ],
    "testRunner": "<rootDir>/node_modules/jest-cli/src/testRunners/jasmine/jasmine2.js",
    "unmockedModulePathPatterns": [
      "react",
      "react-dom",
      "react-addons-test-utils",
      "fbjs",
      "enzyme",
      "cheerio",
      "htmlparser2",
      "lodash",
      "domhandler",
      "object.assign",
      "define-properties",
      "function-bind",
      "object-keys",
      "object.values",
      "es-abstract"
    ],
    "testPathIgnorePatterns": [
      "/node_modules/",
      "/fakeData/"
    ],
    "setupTestFrameworkScriptFile": "./tests/init.js"
  }
}
